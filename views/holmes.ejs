<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <title>LightSource</title>
        <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
        <link rel="stylesheet/less" type="text/css" href="/holmes/css/less/app.less" />
        <script src="/bower_components/less/dist/less-1.7.5.js" type="text/javascript"></script>
        <script type="text/javascript"> less = { env: "development" }; </script>
        <script src="/holmes/js/debugger/index.js" type="text/javascript"></script>
    </head>
    <body>
      <header id="header" class="proto-header">
        <div class='col-xs-12 proto-div'>
          <div class='proto-run col-xs-2'>
            <ul class='nav navbar-nav action-buttons'>
              <li data-action='Run'><a href='javascript:void(0)'><i class="fa fa-play-circle"></i> Run</a></li>
            </ul>
          </div>
          <div class='proto-scrubber col-xs-9'>
            <div id='scrubber' class='col-xs-12'></div>
          </div>
        </div>
      </header>
      <div class='col-xs-12' style='height:100%;'>
          <div id='editor' class='app-boxes col-xs-6'>
            <div class='tabs editor-tabs col-xs-12'>
                <ul class="nav navbar-nav files">
                  <li class="active" data-filename="index.js"><a href="javascript:void(0)">index.js</a></li>
                  <li data-filename="index.html"><a href="javascript:void(0)">index.html</a></li>
                </ul>
            </div>
            <textarea id="lightsource-text"></textarea>
          </div>
          <div class="col-xs-6 app-boxes">
            <div class='tabs scope-tabs col-xs-12'>
                <ul class="nav navbar-nav scopes">
                  <li data-stack="call-stack"><a href="javascript:void(0)">Call Stack</a></li>
                  <li class="active" data-stack="scope"><a href="javascript:void(0)">Scope</a></li>
                </ul>
            </div>
              <table class="call-stack table hidden"><tr></tr></table>
              <table class="var-scope table "><tr></tr></table>
          </div>
          <div class='col-xs-6'>
            <div id='browser' class=' col-xs-12'></div>
          </div>
        </div>
        <script src="/holmes/js/app-built.js"></script>
    </body>
</html>
