<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <title>LightSource</title>

        <link rel="stylesheet/less" type="text/css" href="/holmes/css/less/app.less" />
        <script src="/bower_components/less/dist/less-1.7.5.js" type="text/javascript"></script>
        <script type="text/javascript"> less = { env: "development" }; </script>
        <script src="/holmes/js/debugger/index.js" type="text/javascript"></script>
    </head>
    <body>
        <header class="navbar navbar-default " role="banner">
          <div class="container">
            <div class="navbar-header">
              <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
              <a class="navbar-brand" href="#">debug.js</a>
            </div>
          </div>
        </header>
        <div class='col-xs-12'>
          <div class='scrubber-box col-xs-6'>
            <div class='button-box col-xs-2'>
              <button id='play-pause' class='play'>Play</button>
            </div>
            <div id='scrubber' class='col-xs-10'></div>
          </div>
        </div>
        <div class='col-xs-12' style='height:100%;'>
          <div id='editor' class='app-boxes col-xs-6'>
            <div class='editor-tabs col-xs-12'>
                <ul class="nav navbar-nav files">
                  <li data-filename="Run"><a href="javascript:void(0)">Run</a></li>
                  <li class="active" data-filename="index.js"><a href="javascript:void(0)">index.js</a></li>
                  <li data-filename="index.html"><a href="javascript:void(0)">index.html</a></li>
                </ul>
            </div>
            <textarea id="lightsource-text"></textarea>
          </div>
          <div class="panel panel-default col-xs-6">
              <div class="panel-heading">
                <div class="panel panel-default">
                  <div class="panel-heading stack-heading"><h6>Call Stack</h6></div>
                  <table class="call-stack table table-striped"><tr></tr></table>
                </div>
                <div class="panel panel-default">
                  <div class="panel-heading scope-heading"><h6>Scope</h6></div>
                  <table class="var-scope table table-striped"><tr></tr></table>
                </div>
              </div>
              <div id='browser' class='app-boxes col-xs-12'></div>
          </div>
        </div>
        <script src="/holmes/js/app-built.js"></script>
    </body>
</html>
